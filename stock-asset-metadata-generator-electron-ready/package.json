{
  "name": "stock-asset-metadata-generator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist",
    "electron": "electron .",
    "dist": "electron-builder",
    "dist:electron": "electron-builder --win portable"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "@google/genai": "^1.16.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "gh-pages": "^6.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.6.0"
  },
  "main": "electron.js",
  "build": {
    "appId": "com.example.stockasset",
    "productName": "Stock Asset Metadata Generator",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron.js",
      "preload.js",
      "package.json"
    ],
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}